{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "commonjs": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "requireConfigFile": false
  },
  "rules": {
    "semi": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
